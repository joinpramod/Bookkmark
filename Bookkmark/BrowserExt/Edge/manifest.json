{
    "author": "Booqmarqs",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "browser_action": {
        "default_icon": {
            "48": "icon.png"
        },
        "default_popup": "popup.html"
    },
    "description": "Extension for booqmarqs.com",
    "icons": {
        "48": "icon.png"
    },
    "manifest_version": 2,
    "name": "Booqmarqs",
    "permissions": [
        "tabs",
        "<all_urls>",
        "bookmarks",
        "cookies"
    ],
    "version": "1.6",
    "-ms-preload": {
        "backgroundScript": "backgroundScriptsAPIBridge.js",
        "contentScript": "contentScriptsAPIBridge.js"
    }
}