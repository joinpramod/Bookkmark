<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="expires" content="0" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="Language" content="en-us" />
    <meta name="Robots" content="NoIndex" />
    <meta name="application-name" content="Booqmarqs" />
    <meta name="keywords" content="Online Bookmarks, online bookmarks manager, browser extensions, import bookmarks, free online bookmarks, tree view, increase web traffic, build user base, site value" />
    <meta name="description" content="Online bookmarks with browser extensions, tree view, import bookmarks, publisher plugins to increase web traffic, site value and build user base">
    <title>Booqmarqs - Online bookmarks with browser extensions, tree view, import bookmarks, publisher plugins to increase web traffic, site value and build user base</title>
    
    <link rel="shortcut icon" href="~/images/favicon.png">
    <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap-theme.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/themes/base/jquery-ui.min.css" />
    <link rel="stylesheet" href="~/Content/plugins.min.css">
    <link rel="stylesheet" href="~/Content/pe-icon-7-stroke.css">
    <link rel="stylesheet" href="~/Content/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/style.css">
    <link rel="stylesheet" href="~/Content/responsive.css">

    <script src="~/Scripts/jquery-2.2.4.min.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    <script src="~/Scripts/platform.js" async defer type="text/javascript"></script>
    <script src="~/Scripts/JavaScript.1.3.js" async defer type="text/javascript"></script>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-112287387-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-112287387-1');
    </script>

    @*<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <script>
      (adsbygoogle = window.adsbygoogle || []).push({
        google_ad_client: "ca-pub-7757257431735868",
        enable_page_level_ads: true
      });
    </script>*@
</head>
<body class="default-page">

    <div id="fb-root"></div>
    <script>
            (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/en_US/all.js#xfbml=1";
                fjs.parentNode.insertBefore(js, fjs);
            }
     (document, 'script', 'facebook-jssdk'));</script>
    <script type="text/javascript" async src="https://platform.twitter.com/widgets.js"></script>

    <div class="wrapper">
        <header class="header_area">
            <div class="main_header_area animated">
                <div class="container">
                    <div class="row">

                        <div class="col-sm-4">
                            <div class="my_logo_area">
                                <a href="@Url.Action("Index", "Home")"><img src="~/images/logo.png" alt="Booqmarqs"><span>Booqmarqs</span> </a>
                            </div>
                        </div>

                        <div class="col-sm-8 text-right">
                            <div class="main_menu_area d-inline-block">
                                <div class="mainmenu">
                                    <nav>
                                        <ul id="nav">
                                            @if (Session["User"] == null)
                                            {
                                                <li><a href="/Home/Index#home">Home</a></li>
                                                <li><a href="/Home/Index#howitworks">How It Works</a></li>
                                                <li><a href="/Home/Index#whattoexpect">What To Expect</a></li>
                                                @*<li><a href="@Url.Action("Demo", "Home")">Demo</a></li>*@
                                                <li><a href="@Url.Action("Login", "Account")">Login</a></li>
                                                <li><a href="@Url.Action("Choice", "Account")">Get Started</a></li>
                                            }
                                            else
                                            {
                                                Users user = (Users)Session["User"];
                                                if (user.IsPublisher)
                                                {
                                                    <li>@Html.ActionLink("Reports", "Reports", "Bookmark")</li>
                                                    <li>@Html.ActionLink("Script", "ScriptCode", "Bookmark")</li>
                                                    <li>@Html.ActionLink("Domains", "Domains", "Bookmark")</li>
                                                }
                                                <li>@Html.ActionLink("Bookmarks", "MyBookmarks", "Bookmark")</li>
                                                <li>@Html.ActionLink("Import", "Import", "Bookmark")</li>
                                                <li>@Html.ActionLink("Extensions", "Extensions", "Bookmark")</li>
                                                @*<li><a href="@Url.Action("Demo", "Home")">Demo</a></li>*@
                                                <li>@Html.ActionLink("Profile", "ViewUser", "Account")</li>
                                                <li>@Html.ActionLink("Logout", "Logout", "Account")</li>
                                            }
                                        </ul>
                                    </nav>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Main Header Area End -->
        </header>

        <div style="height:80px"></div>
        @RenderBody()
        <div class="push"></div>

    </div>


    <footer class="footer">
        <div class="footer_bottom_area">
            <div class="container">
                <div class="row">
                    <div class="col-6">
                        <div class="footer_menu text-left">
                            <ul>
                                <li><a href="@Url.Action("API", "Home")">API</a></li>
                                <li><a href="@Url.Action("Policy", "Home")">Privacy Policy</a></li>
                                @*<li><a href="@Url.Action("Disclaimer", "Home")">Term &amp; Conditions</a></li>
                                    <li><a href="@Url.Action("Contact", "Home")">Help or Feedback</a></li>*@
                                <li><a href="@Url.Action("Contact", "Home")">Contact Us</a></li>
                            </ul>
                            <div class="footer_social_area">
                                <a href="https://www.facebook.com/sharer/sharer.php?u=booqmarqs.com"><i class="fa fa-facebook"></i></a>
                                <a href="https://plus.google.com/share?url=http://booqmarqs.com"><i class="fa fa-google-plus"></i></a>
                                @*<a href="#"><i class="fa fa-pinterest"></i></a>*@
                                <a href="https://twitter.com/share?url=http://booqmarqs.com"><i class="fa fa-twitter"></i></a>
                            </div>
                        </div>
                    </div>
                    <div class="col-6">
                        <div class="footer_menu text-right">
                            <div class="footer_bottom text-right copywrite-area">
                                Copyright © 2017-2018 Booqmarqs. All rights reserved.
                            </div>
                            <div style="padding-top:7px" class="fb-like" data-href="https://facebook.com/booqmarqs" data-layout="button_count" data-action="like" data-size="large" data-show-faces="false" data-share="false"></div>
                            <div style="padding-top:7px">
                                <a href="https://twitter.com/booqmarqs" class="twitter-follow-button" data-size="large" data-show-screen-name="false" data-lang="en" data-show-count="true">Follow</a>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>

    </footer>


    <script src="~/Scripts/popper.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/owl.carousel.min.js"></script>
    <script src="~/Scripts/plugins.min.js"></script>
    <script src="~/Scripts/custom.js"></script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
